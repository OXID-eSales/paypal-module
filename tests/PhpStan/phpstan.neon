            message: "#^Method\\s?OxidSolutionCatalysts\\\\PayPal\\\\Service\\\\ModuleSettings::save\\(\\)\\s?has\\s?parameter\\s?\\$value\\s?with\\s?no\\s?type\\s?specified.$#"
            count: 1
            path: ../../src/Service/ModuleSettings.php
    bootstrapFiles:
        - phpstan-bootstrap.php
    level: max
    scanFiles:
        - ../../vendor/oxid-esales/oxideshop-ce/source/oxfunctions.php
        - ../../vendor/oxid-esales/oxideshop-ce/source/overridablefunctions.php
includes:
    - phpstan-baseline.neon